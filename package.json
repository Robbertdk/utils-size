{
  "name": "suitcss-utils-size",
  "description": "Sizing utilities for SUIT CSS",
  "version": "0.6.2",
  "style": "index.css",
  "files": [
    "index.css",
    "lib"
  ],
  "devDependencies": {
    "suitcss-preprocessor": "~0.3.0",
    "suitcss-components-test": "*"
  },
  "scripts": {
